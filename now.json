 
{
    "version": 2,
    "builds": [
        {
        "src": "index.js",
        "use": "@now/node"
        }
    ],
    "env": {
      "CON_STRING": "@con_string"
    },
    "routes": [{
        "headers": {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
            "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
        },
        "src": "/dist/.*", "dest": "index.js"
    }]
}